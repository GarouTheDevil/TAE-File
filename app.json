{
    "name": "Bitly Link Shortener",
    "description": "Bot can Generate Bitly short l8nk ",
    "repository": "https://github.com/lntechnical2/BitlyBot",
    "keywords": ["Bitly", "pyrogram", "telegram bot"],
    "env": {
        "TOKEN": {
            "description": "Your Bot token from @Botfather"
            "required": false
        },
        "API_ID": {
            "description": "Your API_ID from https://my.telegram.org/apps"
            "required": false
        },
        "API_HASH": {
            "description": "Your API_HASH from https://my.telegram.org/apps"
            "required": false
        },
        "BITLY_TOKEN":{
            "description": "Your BITLY_TOKEN from https://bitly.is/accesstoken"
            "required": false
        
        }
    },
    "buildpacks": [{
        "url": "heroku/python"
    }],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "eco"
        }
    },
    "stack": "heroku-20"
}
